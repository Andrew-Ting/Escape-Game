//Inventory opening and closing + inventory items appear
$(document).ready(function () {
$("#arrowup").click (function () {
$("#storagearea, #arrowdown").show();
$("#arrowup").hide();
if (a==1) {
$("#bedinventory").show();
}
if (a==2) {
$("#bedinventory").hide();
}

if (b==1) {
$(".weakeninvent").show();
if (c==1) {
$(".weakeninvent").hide();
}
}
if (d==1) {
$(".barinvent").show();
}
if (e==1) {
$("#paperinvent").show();
}
});
$("#arrowdown").click( function () {
$("#storagearea,#arrowdown").hide();
$("#bedinventory, .weakeninvent, .barinvent, #paperinvent").hide();
$("#arrowup").show();
});
});



//Movement between areas via hole
$(document).ready(function () {
$("#escaperect").click(function() {
$("#begin, #toilet, #bed, #brokenbar, #escaperect").hide();
$("#sidearrowkeys, #begin2").show();
thelocation=1;
});
});

//Movement between areas via arrows
$(document).ready(function () {
$("#arrowleft").click(function () {

if (thelocation==0) {
$("#barholecom, #bedcom, #barweakcom,  #toiletcom, #pillowmattress, #bedinventcom, #defaultcom, #pillowmattress2, #toiletcom2, #barweakcom3, #barweakcom2, #weakeninventcom, #weakenbotcom, #barram, #barinventcom, #brokenbarcom, #paintingcom, #hiddencom, #hiddencom2, #opencom, #vasecom, #papercom, #jailbarcom, #freedoorcom, #unpreparedcom, #personcom, #smashcom").hide();
$("#wallhitcom").show();
}

if (thelocation==1) {
$("#begin, #toilet, #bed, #escaperect").show();
$("#begin2").hide();
if (d!=1) {
$("#brokenbar").show();
}
thelocation=0;
}

if (thelocation==2) {
$("#begin2, #arrowright").show();
$("#begin3").hide();
thelocation=1;
}

if (thelocation==3) {
$("#begin3").show();
$("#finalarea").hide();
thelocation=2;
}
});
});

$(document).ready (function () {
$("#arrowright").click(function () {
if (thelocation==1) {
$("#begin2, #arrowright").hide();
$("#begin3").show();
thelocation=2;
}

if (thelocation==0) {
$("#begin, #toilet, #bed, #brokenbar, #escaperect").hide();
$("#begin2").show();
thelocation=1;
}

});
});

//Code for pathetic prompts :(
$(document).ready(function() {
$("#safe").click(function () {
var x=prompt("4 numbers went together. \n And like hay hiding feather, \n They're hidden deep from weather. \n The biggest lay very last. \n Smallest lay as seen, steadfast. \n The middles did not choose fast. \n They chose the last two, acast.\n A clue in truth now thou hast.");

if (x=="4156") {
$("#barholecom, #bedcom, #barweakcom,  #toiletcom, #pillowmattress, #bedinventcom, #defaultcom, #pillowmattress2, #toiletcom2, #barweakcom3, #barweakcom2, #weakeninventcom, #weakenbotcom, #barram, #barinventcom, #brokenbarcom, #paintingcom, #hiddencom, #hiddencom2, #wallhitcom, #papercom, #freedoorcom, #jailbarcom, #unpreparedcom, #personcom").hide();
$("#opencom, #newsafe").show();
$("#safe").hide();
safepast=1;
}
else{
alert("That wasn't the right code...");
}
});
$("#door").click(function() {
var l=prompt("The door demands some sort of passcode. Engraved on it are the words...\"Safe is key\" \n\n User:");
if(l=="DWI") {
$("#barholecom, #bedcom, #barweakcom,  #toiletcom, #pillowmattress, #bedinventcom, #defaultcom, #pillowmattress2, #toiletcom2, #barweakcom3, #barweakcom2, #weakeninventcom, #weakenbotcom, #barram, #barinventcom, #brokenbarcom, #paintingcom, #hiddencom, #hiddencom2, #wallhitcom, #papercom, #freedoorcom, #jailbarcom, #unpreparedcom, #personcom, #opencom").hide();
$("#door").hide();
$("#opendoor, #freedoorcom").show();
}
else {
alert("Entry denied.");
}
});
});

//Paper check
$(document).ready(function () {

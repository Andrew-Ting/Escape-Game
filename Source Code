$("#bedinventory").fadeOut(500);
a=2;
}
else {
$("#barweakcom3").show();
}
$("svg").off("click", "#pdisab, #mdisab, #barhole", defaulted);
$("svg").off("click","#barweak", barweakcomment);
$("svg").off("click","#toilet", toiletcomment);
}



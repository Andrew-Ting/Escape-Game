$("#paperriddle").show();
$("#paperriddle").click(function () {
$("#paperriddle").hide();
});
});
});

//Final Area
$(document).ready(function() {
$("#opendoor").click(function () {
if (cutscene==0) {
if (d==0) {
$("#barholecom, #bedcom, #barweakcom,  #toiletcom, #pillowmattress, #bedinventcom, #defaultcom, #pillowmattress2, #toiletcom2, #barweakcom3, #barweakcom2, #weakeninventcom, #weakenbotcom, #barram, #barinventcom, #brokenbarcom, #paintingcom, #hiddencom, #hiddencom2, #wallhitcom, #papercom, #freedoorcom, #jailbarcom, #opencom, #personcom").hide();
$("#unpreparedcom").show();
}
else {
$("#begin3").hide();
$("#barholecom, #bedcom, #barweakcom,  #toiletcom, #pillowmattress, #bedinventcom, #defaultcom, #pillowmattress2, #toiletcom2, #barweakcom3, #barweakcom2, #weakeninventcom, #weakenbotcom, #barram, #barinventcom, #brokenbarcom, #paintingcom, #hiddencom, #hiddencom2, #wallhitcom, #papercom, #freedoorcom, #jailbarcom, #opencom, #unpreparedcom, #arrowright").hide();
$("#personcom").show();
$("#finalarea").show();
$("svg").on("click", "#arrowleft, #arrowright, #paperinvent", stuck);
$("svg").one("click", "#brokebarinvent", usingthebar);
}
}
if (cutscene==1) {
$("#finalarea").show();
$("#begin3").hide();
thelocation=3;
}
});
});

function stuck() {
$("#personcom, #paperriddle, #wallhitcom, #escapecom, #arrowright, #barinventcom").hide();
$("#stuck").show();
}

function usingthebar () {
$("#personcom, #paperriddle, #wallhitcom, #stuck, #escapecom").hide();
$("#guy").click(function() {
$("#guy, #barinventcom, #stuck, #escapecom").hide();
$("#smashcom").show();
thelocation=3;
cutscene=1;
$("svg").off("click", "#arrowleft, #arrowright, #paperinvent", stuck);
$("svg").off("click", "#pdisab, #mdisab, #barhole, #toilet, #barweak, #table, #painting", defaulted3);
$("svg").off("click","#code", codeuse);
});
}


//Endgame-ish
$(document).ready(function() {
$("#escape").click(function() {
if (cutscene==1) {
complete=1;
$("#barholecom, #bedcom, #barweakcom,  #toiletcom, #pillowmattress, #bedinventcom, #defaultcom, #pillowmattress2, #toiletcom2, #barweakcom3, #barweakcom2, #weakeninventcom, #weakenbotcom, #barram, #barinventcom, #brokenbarcom, #paintingcom, #hiddencom, #hiddencom2, #wallhitcom, #papercom, #freedoorcom, #jailbarcom, #opencom, #unpreparedcom, #personcom, #smashcom").hide();
$("#endcom").show();
$("#lighten").fadeIn(2000);
setTimeout(function() {$("#endtitle").fadeIn(2000);}, 2000);
setTimeout(function() {$("#titletext").show();}, 4500);
setTimeout(achievements, 5000);
setTimeout(function() {$("#instruct").show();}, 7000);
}
else {
$("#personcom, #wallhitcom, #stuck, #barinventcom").hide();
$("#escapecom").show();
}
});
});

//Achievements
function achievements () {
$("#btitle4").fadeIn(2000);

if (ach1==0) {
$("#btitle1").fadeIn(2000);
}
else {
$("#title1").fadeIn(2000);
}
if (codepast==0 && safepast==0 && complete==1) {
$("#title2").fadeIn(2000);
}
else {
$("#btitle2").fadeIn(2000);
}

if (ach5==0) {
$("#btitle5").fadeIn(2000);
}
else {
$("#title5").fadeIn(2000);
}
if (ach6==0) {
$("#btitle6").fadeIn(2000);
}
else {
$("#title6").fadeIn(2000);
}
}

//Some Achievement Requirements
$(document).ready(function() {
$(".start").click(function() {
var timer=setTimeout(function() {ach3=1;}, 300000);
$("#escape").click(function() {
if (complete==1 && ach3==0) {
setTimeout(function() {$("#title3").fadeIn(2000);}, 5000);
clearTimeout(timer);
}
if (complete==1 && ach3==1) {
setTimeout(function() {$("#btitle3").fadeIn(2000);}, 5000);
}
});
});
$("#arrowleft").click(function() {
if(cutscene==1) {
ach6=1;
}
});
});

//Click the titles or achievements
$(document).ready(function() {
$("#title1").click(function() {
alert("Obtained by attempting to place the bedframe on the toilet.");
});
$("#title2").click(function() {
alert("Obtained by finishing the game without opening the safe or the hidden compartment.");
});
$("#title3").click(function() {
alert("Obtained by finishing the game in or under 5 minutes.");
});
$("#title4").click(function() {
alert("Obtained by clicking the instruction below.");
});
$("#title5").click(function() {
alert("Obtained by checking the credits page before starting the game.");
});
$("#title6").click(function() {
alert("Obtained by still going back inside the prison campus after defeating the gang member.");
});
$("#instruct").click(function() {
$("#btitle4").fadeOut(500);
setTimeout(function() {$("#title4").fadeIn(1000);}, 500);
});
});

</script>

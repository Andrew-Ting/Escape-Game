function barweakcomment() {
$("#barholecom, #barweakcom,  #toiletcom, #bedinventcom, #bedcom, #pillowmattress, #defaultcom, #barweakcom3, #toiletcom2, #barweakcom2, #weakeninventcom, #weakenbotcom, #brokenbarcom, #barram, #barinventcom, #wallhitcom, #vasecom, #paintingcom, #hiddencom, #hiddencom2, #opencom, #papercom, #jailbarcom, #freedoorcom, #unpreparedcom, #smashcom").hide();
